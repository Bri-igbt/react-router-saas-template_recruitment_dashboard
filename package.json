  "name": "reactsquad-react-router-saas-template",
  "private": true,
  "scripts": {
    "build": "react-router build",
    "build-with-mocks": "react-router build && npx msw init ./build/client --no-save",
    "dev": "react-router dev",
    "dev-with-mocks": "cross-env VITE_CLIENT_MOCKS=true SERVER_MOCKS=true npm run dev",
    "dev-with-server-mocks": "cross-env SERVER_MOCKS=true npm run dev",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky || true",
    "prisma:deploy": "npx prisma migrate deploy && npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name",
    "prisma:push": "npx prisma db push && npx prisma generate",
    "prisma:reset-dev": "npm run prisma:wipe && npm run prisma:seed && npm run dev",
    "prisma:reset-dev-with-mocks": "npm run prisma:wipe && npm run prisma:seed && npm run dev-with-mocks",
    "prisma:seed": "tsx ./prisma/seed.ts",
    "prisma:setup": "prisma generate && prisma migrate deploy && prisma db push",
    "prisma:studio": "npx prisma studio",
    "prisma:wipe": "npx prisma migrate reset --force && npx prisma db push",
    "start": "react-router-serve ./build/server/index.js",
    "start-with-server-mocks": "cross-env SERVER_MOCKS=true npm run start",
    "test": "vitest --reporter=verbose",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "typecheck": "react-router typegen && tsc",
    "typegen": "react-router typegen"
  },
  "type": "module"
}
